<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<VBox alignment="CENTER" spacing="10" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml" fx:controller="tn.library_managment_system.Controller.LoginController">
        <TextField fx:id="emailTextField" prefHeight="26.0" prefWidth="200.0" text="email" />
        <Separator prefWidth="200.0" />
        <PasswordField fx:id="passwordTextField" prefHeight="26.0" prefWidth="200.0" text="password" />
        <Text fx:id="cinErrorText" />
        <Separator prefWidth="200.0" />
        <Button mnemonicParsing="false" prefHeight="26.0" prefWidth="98.0" text="Login" onAction="#handleLogin" />
        <Button fx:id="signInButton" text="Sign In" onAction="#redirectToSignInScreen"/>
    </VBox>
